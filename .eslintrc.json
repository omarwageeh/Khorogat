{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "indent":"off",

        "linebreak-style": [
            "error",
            "windows"
        ],
        "quotes": "off",
        "no-console": "warn",
        "semi": "off",

        "no-unused-vars": "off",
        
        "no-extra-semi":"off"
        
    }
}